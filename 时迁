-- æ—¶è¿è„šæœ¬ v3.0 - ç¬¬äºŒéƒ¨åˆ†ï¼šè‡ªçž„ä¸Žæ­¦å™¨åŠŸèƒ½
-- æŽ¥ç¬¬ä¸€éƒ¨åˆ†ä»£ç 

-- åˆ›å»ºMD3é£Žæ ¼å¼€å…³ç»„ä»¶
local function CreateMD3Switch(container, label, initialState, callback)
    local switchFrame = Instance.new("Frame")
    switchFrame.Name = label .. "SwitchFrame"
    switchFrame.Size = UDim2.new(1, 0, 0, 50)
    switchFrame.BackgroundTransparency = 1
    switchFrame.LayoutOrder = #container:GetChildren()
    switchFrame.Parent = container
    
    local switchContainer = Instance.new("Frame")
    switchContainer.Name = "SwitchContainer"
    switchContainer.Size = UDim2.new(1, 0, 1, 0)
    switchContainer.BackgroundTransparency = 1
    switchContainer.Parent = switchFrame
    
    -- MD3å¼€å…³è½¨é“
    local switchTrack = Instance.new("Frame")
    switchTrack.Name = "SwitchTrack"
    switchTrack.Size = UDim2.new(0, 52, 0, 32)
    switchTrack.Position = UDim2.new(0, 0, 0.5, -16)
    switchTrack.BackgroundColor3 = initialState and MD3Colors.Primary or Color3.fromRGB(121, 116, 126, 0.3)
    switchTrack.BorderSizePixel = 0
    
    local trackCorner = Instance.new("UICorner")
    trackCorner.CornerRadius = UDim.new(1, 0)
    trackCorner.Parent = switchTrack
    
    -- MD3å¼€å…³æ‰‹æŸ„
    local switchHandle = Instance.new("Frame")
    switchHandle.Name = "SwitchHandle"
    switchHandle.Size = UDim2.new(0, 24, 0, 24)
    switchHandle.Position = initialState and UDim2.new(0, 24, 0.5, -12) or UDim2.new(0, 4, 0.5, -12)
    switchHandle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    switchHandle.BorderSizePixel = 0
    
    local handleCorner = Instance.new("UICorner")
    handleCorner.CornerRadius = UDim.new(1, 0)
    handleCorner.Parent = switchHandle
    
    -- å¼€å…³é˜´å½±
    local handleShadow = Instance.new("UIStroke")
    handleShadow.Color = Color3.fromRGB(0, 0, 0, 0.1)
    handleShadow.Thickness = 1
    handleShadow.Parent = switchHandle
    
    -- MD3å¼€å…³æŒ‰é’®
    local switchButton = Instance.new("TextButton")
    switchButton.Name = "SwitchButton"
    switchButton.Size = UDim2.new(0, 52, 0, 32)
    switchButton.Position = UDim2.new(0, 0, 0.5, -16)
    switchButton.BackgroundTransparency = 1
    switchButton.Text = ""
    switchButton.AutoButtonColor = false
    switchButton.Parent = switchContainer
    
    -- å¼€å…³æ ‡ç­¾
    local switchLabel = Instance.new("TextLabel")
    switchLabel.Name = "SwitchLabel"
    switchLabel.Size = UDim2.new(1, -60, 1, 0)
    switchLabel.Position = UDim2.new(0, 60, 0, 0)
    switchLabel.BackgroundTransparency = 1
    switchLabel.Text = label
    switchLabel.TextColor3 = MD3Colors.OnSurface
    switchLabel.TextSize = 16
    switchLabel.Font = Enum.Font.Gotham
    switchLabel.TextXAlignment = Enum.TextXAlignment.Left
    switchLabel.TextYAlignment = Enum.TextYAlignment.Center
    switchLabel.Parent = switchContainer
    
    switchTrack.Parent = switchContainer
    switchHandle.Parent = switchContainer
    
    local state = initialState
    
    -- å¼€å…³åŠ¨ç”»
    local function AnimateSwitch(newState)
        state = newState
        
        if newState then
            local trackColorTween = TweenService:Create(switchTrack, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                BackgroundColor3 = MD3Colors.Primary
            })
            
            local handleMoveTween = TweenService:Create(switchHandle, TweenInfo.new(0.2, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
                Position = UDim2.new(0, 24, 0.5, -12)
            })
            
            trackColorTween:Play()
            handleMoveTween:Play()
        else
            local trackColorTween = TweenService:Create(switchTrack, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                BackgroundColor3 = Color3.fromRGB(121, 116, 126, 0.3)
            })
            
            local handleMoveTween = TweenService:Create(switchHandle, TweenInfo.new(0.2, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
                Position = UDim2.new(0, 4, 0.5, -12)
            })
            
            trackColorTween:Play()
            handleMoveTween:Play()
        end
        
        if callback then
            callback(newState)
        end
    end
    
        
    -- ç‚¹å‡»äº‹ä»¶
    switchButton.MouseButton1Click:Connect(function()
        AnimateSwitch(not state)
    end)
    
    -- æ‚¬åœæ•ˆæžœ
    switchButton.MouseEnter:Connect(function()
        local tween = TweenService:Create(switchHandle, TweenInfo.new(0.2), {
            Size = UDim2.new(0, 26, 0, 26)
        })
        tween:Play()
    end)
    
    switchButton.MouseLeave:Connect(function()
        local tween = TweenService:Create(switchHandle, TweenInfo.new(0.2), {
            Size = UDim2.new(0, 24, 0, 24)
        })
        tween:Play()
    end)
    
    return switchFrame, function() return state end, AnimateSwitch
end

-- åˆ›å»ºMD3æ»‘å—ç»„ä»¶
local function CreateMD3Slider(container, label, min, max, defaultValue, callback)
    local sliderFrame = Instance.new("Frame")
    sliderFrame.Name = label .. "SliderFrame"
    sliderFrame.Size = UDim2.new(1, 0, 0, 70)
    sliderFrame.BackgroundTransparency = 1
    sliderFrame.LayoutOrder = #container:GetChildren()
    sliderFrame.Parent = container
    
    local sliderContainer = Instance.new("Frame")
    sliderContainer.Name = "SliderContainer"
    sliderContainer.Size = UDim2.new(1, 0, 1, 0)
    sliderContainer.BackgroundTransparency = 1
    sliderContainer.Parent = sliderFrame
    
    -- æ ‡ç­¾
    local sliderLabel = Instance.new("TextLabel")
    sliderLabel.Name = "SliderLabel"
    sliderLabel.Size = UDim2.new(1, 0, 0, 20)
    sliderLabel.Position = UDim2.new(0, 0, 0, 0)
    sliderLabel.BackgroundTransparency = 1
    sliderLabel.Text = label
    sliderLabel.TextColor3 = MD3Colors.OnSurface
    sliderLabel.TextSize = 16
    sliderLabel.Font = Enum.Font.Gotham
    sliderLabel.TextXAlignment = Enum.TextXAlignment.Left
    sliderLabel.Parent = sliderContainer
    
    -- æ»‘å—è½¨é“
    local track = Instance.new("Frame")
    track.Name = "Track"
    track.Size = UDim2.new(1, 0, 0, 8)
    track.Position = UDim2.new(0, 0, 0, 30)
    track.BackgroundColor3 = Color3.fromRGB(121, 116, 126, 0.3)
    track.BorderSizePixel = 0
    
    local trackCorner = Instance.new("UICorner")
    trackCorner.CornerRadius = UDim.new(1, 0)
    trackCorner.Parent = track
    
    -- æ»‘å—æ´»åŠ¨è½¨é“
    local activeTrack = Instance.new("Frame")
    activeTrack.Name = "ActiveTrack"
    activeTrack.Size = UDim2.new(0, 0, 1, 0)
    activeTrack.BackgroundColor3 = MD3Colors.Primary
    activeTrack.BorderSizePixel = 0
    
    local activeTrackCorner = Instance.new("UICorner")
    activeTrackCorner.CornerRadius = UDim.new(1, 0)
    activeTrackCorner.Parent = activeTrack
    
    -- æ»‘å—æ‰‹æŸ„
    local handle = Instance.new("Frame")
    handle.Name = "Handle"
    handle.Size = UDim2.new(0, 20, 0, 20)
    handle.Position = UDim2.new(0, -10, 0.5, -10)
    handle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    handle.BorderSizePixel = 0
    
    local handleCorner = Instance.new("UICorner")
    handleCorner.CornerRadius = UDim.new(1, 0)
    handleCorner.Parent = handle
    
    -- æ‰‹æŸ„é˜´å½±
    local handleShadow = Instance.new("UIStroke")
    handleShadow.Color = Color3.fromRGB(0, 0, 0, 0.1)
    handleShadow.Thickness = 1
    handleShadow.Parent = handle
    
    -- å€¼æ˜¾ç¤º
    local valueLabel = Instance.new("TextLabel")
    valueLabel.Name = "ValueLabel"
    valueLabel.Size = UDim2.new(0, 40, 0, 20)
    valueLabel.Position = UDim2.new(1, -40, 0, 0)
    valueLabel.BackgroundTransparency = 1
    valueLabel.Text = tostring(defaultValue)
    valueLabel.TextColor3 = MD3Colors.OnSurfaceVariant
    valueLabel.TextSize = 14
    valueLabel.Font = Enum.Font.Gotham
    valueLabel.TextXAlignment = Enum.TextXAlignment.Right
    valueLabel.Parent = sliderContainer
    
    -- è®¾ç½®åˆå§‹å€¼
    local value = math.clamp(defaultValue, min, max)
    local range = max - min
    local percentage = (value - min) / range
    
    activeTrack.Parent = track
    handle.Parent = track
    track.Parent = sliderContainer
    
    -- æ›´æ–°æ»‘å—ä½ç½®
    local function UpdateSlider(posX)
        local absoluteSize = track.AbsoluteSize.X
        local relativeX = math.clamp(posX, 0, absoluteSize)
        local newPercentage = relativeX / absoluteSize
        local newValue = min + (range * newPercentage)
        newValue = math.floor(newValue * 10) / 10  -- ä¿ç•™ä¸€ä½å°æ•°
        
        activeTrack.Size = UDim2.new(newPercentage, 0, 1, 0)
        handle.Position = UDim2.new(newPercentage, -10, 0.5, -10)
        valueLabel.Text = tostring(newValue)
        
        if callback then
            callback(newValue)
        end
        
        return newValue
    end
    
    -- è®¾ç½®åˆå§‹ä½ç½®
    UpdateSlider(track.AbsoluteSize.X * percentage)
    
    -- æ‹–åŠ¨åŠŸèƒ½
    local dragging = false
    track.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            
            local handleTween = TweenService:Create(handle, TweenInfo.new(0.1), {
                Size = UDim2.new(0, 24, 0, 24)
            })
            handleTween:Play()
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local mousePos = UserInputService:GetMouseLocation()
            local trackPos = track.AbsolutePosition
            value = UpdateSlider(mousePos.X - trackPos.X)
        end
    end)
    
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
            
            local handleTween = TweenService:Create(handle, TweenInfo.new(0.1), {
                Size = UDim2.new(0, 20, 0, 20)
            })
            handleTween:Play()
        end
    end)
    
    return sliderFrame, function() return value end, UpdateSlider
end

-- è‡ªçž„ç³»ç»Ÿ
local AimConfig = {
    Enabled = false,
    Smoothness = 0.15,
    FOV = 120,
    AimKey = Enum.UserInputType.MouseButton2,
    TeamCheck = true,
    WallCheck = true,
    MaxDistance = 1000,
    Prediction = 0.12,
    AimPart = "Head"
}

local AimTarget = nil
local AimConnection = nil

-- èŽ·å–æœ€è¿‘å¯è§çŽ©å®¶
local function GetClosestVisiblePlayer()
    if not LocalPlayer.Character then return nil, nil end
    
    local localRoot = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not localRoot then return nil, nil end
    
    local closestPlayer = nil
    local closestDistance = AimConfig.MaxDistance
    local closestPart = nil
    
    local cameraPos = Camera.CFrame.Position
    local screenCenter = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
    
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            local head = player.Character:FindFirstChild("Head")
            local root = player.Character:FindFirstChild("HumanoidRootPart")
            
            if humanoid and humanoid.Health > 0 and head and root then
                -- é˜Ÿä¼æ£€æŸ¥
                if AimConfig.TeamCheck and player.Team and LocalPlayer.Team and player.Team == LocalPlayer.Team then
                    continue
                end
                
                -- è·ç¦»æ£€æŸ¥
                local distance = (root.Position - localRoot.Position).Magnitude
                if distance > closestDistance then
                    continue
                end
                
                -- å¯è§æ€§æ£€æŸ¥
                local screenPos, onScreen = Camera:WorldToViewportPoint(head.Position)
                if not onScreen then
                    continue
                end
                
                -- FOVæ£€æŸ¥
                local screenPos2D = Vector2.new(screenPos.X, screenPos.Y)
                local distanceFromCenter = (screenPos2D - screenCenter).Magnitude
                local fovRadius = (AimConfig.FOV / 360) * Camera.ViewportSize.Y
                
                if distanceFromCenter > fovRadius then
                    continue
                end
                
                -- å¢™ä½“æ£€æŸ¥
                if AimConfig.WallCheck then
                    local rayOrigin = cameraPos
                    local rayDirection = (head.Position - rayOrigin).Unit * distance
                    local raycastParams = RaycastParams.new()
                    raycastParams.FilterDescendantsInstances = {LocalPlayer.Character, player.Character}
                    raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
                    raycastParams.IgnoreWater = true
                    
                    local hit = Workspace:Raycast(rayOrigin, rayDirection, raycastParams)
                    if hit then
                        local hitParent = hit.Instance:FindFirstAncestorOfClass("Model")
                        if hitParent ~= player.Character then
                            continue
                        end
                    end
                end
                
                -- æ›´æ–°æœ€è¿‘çŽ©å®¶
                if distance < closestDistance then
                    closestDistance = distance
                    closestPlayer = player
                    closestPart = head
                end
            end
        end
    end
    
    return closestPlayer, closestPart
end

-- å¹³æ»‘æ’å€¼å‡½æ•°
local function SmoothLerp(current, target, alpha)
    return current + (target - current) * math.clamp(alpha, 0, 1)
end

local function SmoothAngleLerp(current, target, alpha)
    local delta = target - current
    
    if delta > math.pi then
        delta = delta - 2 * math.pi
    elseif delta < -math.pi then
        delta = delta + 2 * math.pi
    end
    
    return current + delta * alpha
end

-- è‡ªçž„é€»è¾‘
local function SmoothAim()
    if not AimConfig.Enabled then
        AimTarget = nil
        return
    end
    
    -- æ£€æŸ¥æ˜¯å¦æŒ‰ä¸‹è‡ªçž„é”®
    if not UserInputService:IsMouseButtonPressed(AimConfig.AimKey) then
        AimTarget = nil
        return
    end
    
    -- èŽ·å–ç›®æ ‡
    local player, part = GetClosestVisiblePlayer()
    
    if player and part then
        AimTarget = {player = player, part = part}
        
        -- è®¡ç®—é¢„åˆ¤ä½ç½®
        local targetPosition = part.Position
        if AimConfig.Prediction > 0 and part.Velocity then
            local distance = (targetPosition - Camera.CFrame.Position).Magnitude
            targetPosition = targetPosition + (part.Velocity * AimConfig.Prediction * (distance / 1000))
        end
        
        -- èŽ·å–å½“å‰ç›¸æœºä½ç½®å’Œæ–¹å‘
        local currentCFrame = Camera.CFrame
        local cameraPosition = currentCFrame.Position
        
        -- è®¡ç®—ç›®æ ‡æ–¹å‘
        local targetDirection = (targetPosition - cameraPosition).Unit
        
        -- è®¡ç®—å½“å‰æ–¹å‘
        local currentDirection = currentCFrame.LookVector
        
        -- è®¡ç®—è§’åº¦å·®
        local currentAngles = Vector2.new(
            math.atan2(currentDirection.Y, currentDirection.X),
            math.atan2(currentDirection.Z, math.sqrt(currentDirection.X^2 + currentDirection.Y^2))
        )
        
        local targetAngles = Vector2.new(
            math.atan2(targetDirection.Y, targetDirection.X),
            math.atan2(targetDirection.Z, math.sqrt(targetDirection.X^2 + targetDirection.Y^2))
        )
        
        -- å¹³æ»‘æ’å€¼è§’åº¦
        local smoothAngles = Vector2.new(
            SmoothAngleLerp(currentAngles.X, targetAngles.X, AimConfig.Smoothness),
            SmoothAngleLerp(currentAngles.Y, targetAngles.Y, AimConfig.Smoothness)
        )
        
        -- è½¬æ¢å›žæ–¹å‘å‘é‡
        local smoothDirection = Vector3.new(
            math.cos(smoothAngles.X) * math.cos(smoothAngles.Y),
            math.sin(smoothAngles.Y),
            math.sin(smoothAngles.X) * math.cos(smoothAngles.Y)
        ).Unit
        
        -- åˆ›å»ºå¹³æ»‘çš„CFrame
        local smoothCFrame = CFrame.lookAt(cameraPosition, cameraPosition + smoothDirection)
        
        -- åº”ç”¨å¹³æ»‘ç›¸æœºæ—‹è½¬
        Camera.CFrame = smoothCFrame
    else
        AimTarget = nil
    end
end

-- å¯åŠ¨/åœæ­¢è‡ªçž„å¾ªçŽ¯
local function StartAim()
    if AimConnection then
        AimConnection:Disconnect()
    end
    
    AimConnection = RunService.RenderStepped:Connect(SmoothAim)
    ShowFunctionNotification("è‡ªçž„ç³»ç»Ÿ", "å¹³æ»‘é”å¤´å·²å¯ç”¨")
end

local function StopAim()
    if AimConnection then
        AimConnection:Disconnect()
        AimConnection = nil
    end
    AimTarget = nil
    ShowFunctionNotification("è‡ªçž„ç³»ç»Ÿ", "å¹³æ»‘é”å¤´å·²ç¦ç”¨")
end

-- ACSæžªæ¢°ç³»ç»Ÿ
local ACSFunctions = {
    InfiniteAmmo = false,
    NoReload = false,
    RapidFire = false,
    NoRecoil = false,
    NoSpread = false,
    ModifiedGuns = {},
    
    -- é˜²å›žè°ƒå®‰å…¨å‡½æ•°
    SafeCall = function(self, func, ...)
        local success, result = pcall(func, ...)
        if not success then
            warn("ACSåŠŸèƒ½é”™è¯¯: " .. tostring(result))
            return nil
        end
        return result
    end,
    
    -- æŸ¥æ‰¾ACSæžªæ¢°
    FindACSGun = function(self, model)
        return self:SafeCall(function()
            if not model then return nil end
            
            -- æ£€æµ‹æžªæ¢°å·¥å…·
            if model:IsA("Tool") then
                -- æ£€æŸ¥å¸¸è§æžªæ¢°æ¨¡å—
                local checks = {
                    "ACS_Module",
                    "GunModule", 
                    "WeaponModule",
                    "GunScript",
                    "WeaponScript",
                    "FireModule"
                }
                
                for _, name in ipairs(checks) do
                    local module = model:FindFirstChild(name)
                    if module then
                        return module
                    end
                end
                
                -- æ£€æŸ¥Configurationæ–‡ä»¶å¤¹
                local config = model:FindFirstChild("Configuration")
                if config then
                    return config
                end
                
                -- æ£€æŸ¥åŒ…å«"gun"çš„æ¨¡å—è„šæœ¬
                for _, child in ipairs(model:GetDescendants()) do
                    if child:IsA("ModuleScript") then
                        local nameLower = child.Name:lower()
                        if nameLower:find("gun") or nameLower:find("weapon") or nameLower:find("fire") then
                            return child
                        end
                    end
                end
            end
            
            return nil
        end)
    end,
    
    -- ä¿®æ”¹æžªæ¢°æ•°å€¼
    ModifyGunValue = function(self, gun, property, value)
        return self:SafeCall(function()
            -- æ–¹æ³•1: ç›´æŽ¥ä¿®æ”¹å±žæ€§
            if gun[property] ~= nil then
                gun[property] = value
                return true
            end
            
            -- æ–¹æ³•2: åœ¨Configurationä¸­
            local config = gun:FindFirstChild("Configuration")
            if config then
                local val = config:FindFirstChild(property)
                if val and (val:IsA("NumberValue") or val:IsA("IntValue") or val:IsA("BoolValue")) then
                    if val:IsA("BoolValue") then
                        val.Value = (value > 0)
                    else
                        val.Value = value
                    end
                    return true
                end
            end
            
            -- æ–¹æ³•3: ä¿®æ”¹ModuleScript
            if gun:IsA("ModuleScript") then
                local module = require(gun)
                if type(module) == "table" and module[property] ~= nil then
                    module[property] = value
                    return true
                end
            end
            
            return false
        end)
    end,
    
    -- æ— é™å­å¼¹
    EnableInfiniteAmmo = function(self)
        if self.InfiniteAmmo then return end
        self.InfiniteAmmo = true
        ShowFunctionNotification("æ— é™å­å¼¹", "å¼¹è¯å·²è®¾ç½®ä¸º9999")
        
        coroutine.wrap(function()
            while self.InfiniteAmmo do
                self:SafeCall(function()
                    local player = game.Players.LocalPlayer
                    if not player then return end
                    
                    -- æ£€æŸ¥èƒŒåŒ…
                    if player.Backpack then
                        for _, tool in ipairs(player.Backpack:GetChildren()) do
                            if tool:IsA("Tool") and not self.ModifiedGuns[tool] then
                                local gun = self:FindACSGun(tool)
                                if gun then
                                    self:ModifyGunValue(gun, "MaxAmmo", 9999)
                                    self:ModifyGunValue(gun, "CurrentAmmo", 9999)
                                    self:ModifyGunValue(gun, "Ammo", 9999)
                                    self:ModifyGunValue(gun, "ClipSize", 9999)
                                    self.ModifiedGuns[tool] = true
                                end
                            end
                        end
                    end
                    
                    -- æ£€æŸ¥æ‰‹æŒ
                    if player.Character then
                        local tool = player.Character:FindFirstChildOfClass("Tool")
                        if tool and not self.ModifiedGuns[tool] then
                            local gun = self:FindACSGun(tool)
                            if gun then
                                self:ModifyGunValue(gun, "MaxAmmo", 9999)
                                self:ModifyGunValue(gun, "CurrentAmmo", 9999)
                                self.ModifiedGuns[tool] = true
                            end
                        end
                    end
                end)
                
                wait(0.5)
            end
        end)()
    end,
    
    DisableInfiniteAmmo = function(self)
        self.InfiniteAmmo = false
        self.ModifiedGuns = {}
        ShowFunctionNotification("æ— é™å­å¼¹", "å·²ç¦ç”¨")
    end,
    
    -- å…æ¢å¼¹
    EnableNoReload = function(self)
        if self.NoReload then return end
        self.NoReload = true
        ShowFunctionNotification("å…æ¢å¼¹", "é›¶æ¢å¼¹æ—¶é—´å·²å¯ç”¨")
        
        coroutine.wrap(function()
            while self.NoReload do
                self:SafeCall(function()
                    local player = game.Players.LocalPlayer
                    if not player then return end
                    
                    local function ProcessTool(tool)
                        if not tool or self.ModifiedGuns[tool] then return end
                        local gun = self:FindACSGun(tool)
                        if gun then
                            self:ModifyGunValue(gun, "ReloadTime", 0)
                            self:ModifyGunValue(gun, "ReloadSpeed", 9999)
                            self:ModifyGunValue(gun, "AutoReload", true)
                            self.ModifiedGuns[tool] = true
                        end
                    end
                    
                    if player.Backpack then
                        for _, tool in ipairs(player.Backpack:GetChildren()) do
                            if tool:IsA("Tool") then
                                ProcessTool(tool)
                            end
                        end
                    end
                    
                    if player.Character then
                        local tool = player.Character:FindFirstChildOfClass("Tool")
                        ProcessTool(tool)
                    end
                end)
                
                wait(0.5)
            end
        end)()
    end,
    
    DisableNoReload = function(self)
        self.NoReload = false
        self.ModifiedGuns = {}
        ShowFunctionNotification("å…æ¢å¼¹", "å·²ç¦ç”¨")
    end,
    
    -- è¶…é«˜å°„é€Ÿ
    EnableRapidFire = function(self)
        if self.RapidFire then return end
        self.RapidFire = true
        ShowFunctionNotification("è¶…é«˜å°„é€Ÿ", "å°„é€Ÿå·²æœ€å¤§åŒ–")
        
        coroutine.wrap(function()
            while self.RapidFire do
                self:SafeCall(function()
                    local player = game.Players.LocalPlayer
                    if not player then return end
                    
                    local function ProcessTool(tool)
                        if not tool or self.ModifiedGuns[tool] then return end
                        local gun = self:FindACSGun(tool)
                        if gun then
                            self:ModifyGunValue(gun, "FireRate", 1000)
                            self:ModifyGunValue(gun, "RPM", 60000)
                            self:ModifyGunValue(gun, "Cooldown", 0)
                            self.ModifiedGuns[tool] = true
                        end
                    end
                    
                    if player.Backpack then
                        for _, tool in ipairs(player.Backpack:GetChildren()) do
                            if tool:IsA("Tool") then
                                ProcessTool(tool)
                            end
                        end
                    end
                    
                    if player.Character then
                        local tool = player.Character:FindFirstChildOfClass("Tool")
                        ProcessTool(tool)
                    end
                end)
                
                wait(0.5)
            end
        end)()
    end,
    
    DisableRapidFire = function(self)
        self.RapidFire = false
        self.ModifiedGuns = {}
        ShowFunctionNotification("è¶…é«˜å°„é€Ÿ", "å·²ç¦ç”¨")
    end,
    
    -- æ— åŽååŠ›
    EnableNoRecoil = function(self)
        if self.NoRecoil then return end
        self.NoRecoil = true
        ShowFunctionNotification("æ— åŽååŠ›", "åŽååŠ›å·²æ¶ˆé™¤")
        
        coroutine.wrap(function()
            while self.NoRecoil do
                self:SafeCall(function()
                    local player = game.Players.LocalPlayer
                    if not player then return end
                    
                    local function ProcessTool(tool)
                        if not tool or self.ModifiedGuns[tool] then return end
                        local gun = self:FindACSGun(tool)
                        if gun then
                            self:ModifyGunValue(gun, "Recoil", 0)
                            self:ModifyGunValue(gun, "RecoilKick", 0)
                            self:ModifyGunValue(gun, "RecoilSpread", 0)
                            self.ModifiedGuns[tool] = true
                        end
                    end
                    
                    if player.Backpack then
                        for _, tool in ipairs(player.Backpack:GetChildren()) do
                            if tool:IsA("Tool") then
                                ProcessTool(tool)
                            end
                        end
                    end
                    
                    if player.Character then
                        local tool = player.Character:FindFirstChildOfClass("Tool")
                        ProcessTool(tool)
                    end
                end)
                
                wait(0.5)
            end
        end)()
    end,
    
    DisableNoRecoil = function(self)
        self.NoRecoil = false
        self.ModifiedGuns = {}
        ShowFunctionNotification("æ— åŽååŠ›", "å·²ç¦ç”¨")
    end,
    
    -- æ— æ•£å¸ƒ
    EnableNoSpread = function(self)
        if self.NoSpread then return end
        self.NoSpread = true
        ShowFunctionNotification("æ— æ•£å¸ƒ", "ç²¾åº¦100%")
        
        coroutine.wrap(function()
            while self.NoSpread do
                self:SafeCall(function()
                    local player = game.Players.LocalPlayer
                    if not player then return end
                    
                    local function ProcessTool(tool)
                        if not tool or self.ModifiedGuns[tool] then return end
                        local gun = self:FindACSGun(tool)
                        if gun then
                            self:ModifyGunValue(gun, "Spread", 0)
                            self:ModifyGunValue(gun, "Accuracy", 100)
                            self:ModifyGunValue(gun, "BulletSpread", 0)
                            self.ModifiedGuns[tool] = true
                        end
                    end
                    
                    if player.Backpack then
                        for _, tool in ipairs(player.Backpack:GetChildren()) do
                            if tool:IsA("Tool") then
                                ProcessTool(tool)
                            end
                        end
                    end
                    
                    if player.Character then
                        local tool = player.Character:FindFirstChildOfClass("Tool")
                        ProcessTool(tool)
                    end
                end)
                
                wait(0.5)
            end
        end)()
    end,
    
    DisableNoSpread = function(self)
        self.NoSpread = false
        self.ModifiedGuns = {}
        ShowFunctionNotification("æ— æ•£å¸ƒ", "å·²ç¦ç”¨")
    end,
}

-- ç§»åŠ¨åŠŸèƒ½
local Movement = {
    HighJump = false,
    InfiniteJump = false,
    Speed = false,
    SpeedValue = 50,
    JumpPowerValue = 50,
    
    OriginalWalkSpeed = 16,
    OriginalJumpPower = 50,
    
    -- è¶…é«˜è·³è·ƒ
    EnableHighJump = function(self)
        if self.HighJump then return end
        self.HighJump = true
        self.OriginalJumpPower = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and 
                               LocalPlayer.Character.Humanoid.JumpPower or 50
        
        ShowFunctionNotification("è¶…é«˜è·³è·ƒ", "è·³è·ƒé«˜åº¦å·²å¢žåŠ ")
        
        coroutine.wrap(function()
            while self.HighJump do
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
                    LocalPlayer.Character.Humanoid.JumpPower = self.JumpPowerValue
                end
                wait(0.1)
            end
        end)()
    end,
    
    DisableHighJump = function(self)
        self.HighJump = false
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
            LocalPlayer.Character.Humanoid.JumpPower = self.OriginalJumpPower
        end
        ShowFunctionNotification("è¶…é«˜è·³è·ƒ", "å·²ç¦ç”¨")
    end,
    
    -- æ— é™è·³è·ƒ
    EnableInfiniteJump = function(self)
        if self.InfiniteJump then return end
        self.InfiniteJump = true
        ShowFunctionNotification("æ— é™è·³è·ƒ", "åœ¨ç©ºä¸­å¯æ— é™è·³è·ƒ")
        
        local connection
        connection = UserInputService.JumpRequest:Connect(function()
            if self.InfiniteJump and LocalPlayer.Character then
                local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                end
            end
        end)
        
        -- ä¿å­˜è¿žæŽ¥ä»¥ä¾¿ç¦ç”¨
        self.InfiniteJumpConnection = connection
    end,
    
    DisableInfiniteJump = function(self)
        self.InfiniteJump = false
        if self.InfiniteJumpConnection then
            self.InfiniteJumpConnection:Disconnect()
            self.InfiniteJumpConnection = nil
        end
        ShowFunctionNotification("æ— é™è·³è·ƒ", "å·²ç¦ç”¨")
    end,
    
    -- é€Ÿåº¦
    EnableSpeed = function(self)
        if self.Speed then return end
        self.Speed = true
        self.OriginalWalkSpeed = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") and 
                               LocalPlayer.Character.Humanoid.WalkSpeed or 16
        
        ShowFunctionNotification("é€Ÿåº¦", "ç§»åŠ¨é€Ÿåº¦å·²å¢žåŠ ")
        
        coroutine.wrap(function()
            while self.Speed do
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
                    LocalPlayer.Character.Humanoid.WalkSpeed = self.SpeedValue
                end
                wait(0.1)
            end
        end)()
    end,
    
    DisableSpeed = function(self)
        self.Speed = false
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = self.OriginalWalkSpeed
        end
        ShowFunctionNotification("é€Ÿåº¦", "å·²ç¦ç”¨")
    end,
}

