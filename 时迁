-- æŒ‰é’®åŠ¨ç”»
local function ButtonClickAnimation(button)
    local originalSize = button.Size
    local tweenInfo = TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    
    local shrinkTween = TweenService:Create(button, tweenInfo, {
        Size = originalSize - UDim2.new(0, 4, 0, 4)
    })
    
    local restoreTween = TweenService:Create(button, tweenInfo, {
        Size = originalSize
    })
    
    shrinkTween:Play()
    shrinkTween.Completed:Wait()
    restoreTween:Play()
end

-- å…³é—­æŒ‰é’®åŠŸèƒ½
CloseButton.MouseButton1Click:Connect(function()
    ButtonClickAnimation(CloseButton)
    
    local closeTween = TweenService:Create(MainWindow, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
        Size = UDim2.new(0, 0, 0, 0),
        BackgroundTransparency = 1
    })
    closeTween:Play()
    
    closeTween.Completed:Wait()
    ScreenGui:Destroy()
    ShowFunctionNotification("è„šæœ¬å…³é—­", "æ—¶è¿è„šæœ¬å·²å…³é—­")
end)

-- æœ€å°åŒ–æŒ‰é’®
local isMinimized = false
MinimizeButton.MouseButton1Click:Connect(function()
    ButtonClickAnimation(MinimizeButton)
    
    isMinimized = not isMinimized
    local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    
    if isMinimized then
        local minimizeTween = TweenService:Create(MainWindow, tweenInfo, {
            Size = UDim2.new(0, 150, 0, 50)  -- æœ€å°åŒ–æ—¶åªæ˜¾ç¤ºå¯¼èˆªæ 
        })
        minimizeTween:Play()
        MinimizeButton.Text = "+"
        ContentPanel.Visible = false
    else
        ContentPanel.Visible = true
        local maximizeTween = TweenService:Create(MainWindow, tweenInfo, {
            Size = OriginalSize
        })
        maximizeTween:Play()
        MinimizeButton.Text = "âˆ’"
    end
end)

-- æ‚¬åœæ•ˆæžœ
CloseButton.MouseEnter:Connect(function()
    local tween = TweenService:Create(CloseButton, TweenInfo.new(0.2), {
        TextColor3 = MD3Colors.Error
    })
    tween:Play()
end)

CloseButton.MouseLeave:Connect(function()
    local tween = TweenService:Create(CloseButton, TweenInfo.new(0.2), {
        TextColor3 = MD3Colors.OnSurface
    })
    tween:Play()
end)

MinimizeButton.MouseEnter:Connect(function()
    local tween = TweenService:Create(MinimizeButton, TweenInfo.new(0.2), {
        TextColor3 = MD3Colors.Primary
    })
    tween:Play()
end)

MinimizeButton.MouseLeave:Connect(function()
    local tween = TweenService:Create(MinimizeButton, TweenInfo.new(0.2), {
        TextColor3 = MD3Colors.OnSurface
    })
    tween:Play()
end)

