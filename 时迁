print("âœ… è‡ªçž„ä¸Žæ­¦å™¨åŠŸèƒ½åŠ è½½å®Œæˆ")
print("ç­‰å¾…ç¬¬ä¸‰éƒ¨åˆ†ï¼šä¼˜åŒ–ä¸Žå®Œæˆ...")

-- æ—¶è¿è„šæœ¬ v3.0 - ç¬¬ä¸‰éƒ¨åˆ†ï¼šä¼˜åŒ–ä¸Žå®Œæˆ
-- æŽ¥ç¬¬äºŒéƒ¨åˆ†ä»£ç 

-- çƒ­é”®ç³»ç»Ÿ
local Hotkeys = {
    ToggleUI = Enum.KeyCode.RightShift,  -- å³Shifté”®æ˜¾ç¤º/éšè—UI
    ToggleAim = Enum.KeyCode.F,          -- Fé”®å¼€å…³è‡ªçž„
    ToggleFly = Enum.KeyCode.G,          -- Gé”®å¼€å…³é£žè¡Œ
    ToggleNoClip = Enum.KeyCode.H,       -- Hé”®å¼€å…³ç©¿å¢™
}

-- é£žè¡Œç³»ç»Ÿ
local Fly = {
    Enabled = false,
    Speed = 50,
    FlyConnection = nil,
    NoClip = false,
    NoClipConnection = nil,
    Flying = false,
    Velocity = Vector3.new(0, 0, 0),
    LastPosition = nil,
    
    -- å¯ç”¨é£žè¡Œ
    EnableFly = function(self)
        if self.Enabled then return end
        self.Enabled = true
        
        local character = LocalPlayer.Character
        if not character then return end
        
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if not humanoid then return end
        
        ShowFunctionNotification("é£žè¡Œæ¨¡å¼", "é£žè¡Œå·²å¯ç”¨ (Gé”®å¼€å…³)")
        
        self.Flying = false
        self.Velocity = Vector3.new(0, 0, 0)
        self.LastPosition = character.PrimaryPart.Position
        
        -- é£žè¡ŒæŽ§åˆ¶
        self.FlyConnection = RunService.Heartbeat:Connect(function(delta)
            if not self.Enabled or not character or not character.PrimaryPart then return end
            
            local root = character.PrimaryPart
            local forward = Camera.CFrame.LookVector
            local right = Camera.CFrame.RightVector
            local up = Vector3.new(0, 1, 0)
            
            local moveDirection = Vector3.new(0, 0, 0)
            
            -- WASDæŽ§åˆ¶
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                moveDirection = moveDirection + forward
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                moveDirection = moveDirection - forward
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                moveDirection = moveDirection - right
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                moveDirection = moveDirection + right
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                moveDirection = moveDirection + up
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) or UserInputService:IsKeyDown(Enum.KeyCode.RightControl) then
                moveDirection = moveDirection - up
            end
            
            -- æ ‡å‡†åŒ–ç§»åŠ¨æ–¹å‘
            if moveDirection.Magnitude > 0 then
                moveDirection = moveDirection.Unit
            end
            
            -- åº”ç”¨é€Ÿåº¦
            self.Velocity = self.Velocity:Lerp(moveDirection * self.Speed, 0.5)
            
            -- åº”ç”¨é£žè¡Œ
            if not self.Flying then
                self.Flying = true
                humanoid.PlatformStand = true
            end
            
            -- åº”ç”¨é€Ÿåº¦
            root.Velocity = Vector3.new(0, 0, 0)
            root.CFrame = root.CFrame + self.Velocity * delta
        end)
    end,
    
    -- ç¦ç”¨é£žè¡Œ
    DisableFly = function(self)
        if not self.Enabled then return end
        self.Enabled = false
        
        if self.FlyConnection then
            self.FlyConnection:Disconnect()
            self.FlyConnection = nil
        end
        
        local character = LocalPlayer.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.PlatformStand = false
            end
        end
        
        self.Flying = false
        ShowFunctionNotification("é£žè¡Œæ¨¡å¼", "é£žè¡Œå·²ç¦ç”¨")
    end,
    
    -- å¯ç”¨ç©¿å¢™
    EnableNoClip = function(self)
        if self.NoClip then return end
        self.NoClip = true
        
        ShowFunctionNotification("ç©¿å¢™æ¨¡å¼", "ç©¿å¢™å·²å¯ç”¨ (Hé”®å¼€å…³)")
        
        self.NoClipConnection = RunService.Heartbeat:Connect(function()
            if not self.NoClip then return end
            
            local character = LocalPlayer.Character
            if character then
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") and part.CanCollide then
                        part.CanCollide = false
                    end
                end
            end
        end)
    end,
    
    -- ç¦ç”¨ç©¿å¢™
    DisableNoClip = function(self)
        if not self.NoClip then return end
        self.NoClip = false
        
        if self.NoClipConnection then
            self.NoClipConnection:Disconnect()
            self.NoClipConnection = nil
        end
        
        local character = LocalPlayer.Character
        if character then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
        end
        
        ShowFunctionNotification("ç©¿å¢™æ¨¡å¼", "ç©¿å¢™å·²ç¦ç”¨")
    end,
}

-- çƒ­é”®å¤„ç†
UserInputService.InputBegan:Connect(function(input, processed)
    if processed then return end
    
    -- å³Shifté”®åˆ‡æ¢UIæ˜¾ç¤º
    if input.KeyCode == Hotkeys.ToggleUI then
        MainWindow.Visible = not MainWindow.Visible
        if MainWindow.Visible then
            ShowFunctionNotification("ç•Œé¢æŽ§åˆ¶", "UIå·²æ˜¾ç¤º")
        end
    end
    
    -- Fé”®åˆ‡æ¢è‡ªçž„
    if input.KeyCode == Hotkeys.ToggleAim then
        AimConfig.Enabled = not AimConfig.Enabled
        if AimConfig.Enabled then
            StartAim()
        else
            StopAim()
        end
    end
    
    -- Gé”®åˆ‡æ¢é£žè¡Œ
    if input.KeyCode == Hotkeys.ToggleFly then
        if Fly.Enabled then
            Fly:DisableFly()
        else
            Fly:EnableFly()
        end
    end
    
    -- Hé”®åˆ‡æ¢ç©¿å¢™
    if input.KeyCode == Hotkeys.ToggleNoClip then
        if Fly.NoClip then
            Fly:DisableNoClip()
        else
            Fly:EnableNoClip()
        end
    end
end)

-- å¤´åƒç³»ç»Ÿ
local function SetupAvatar()
    -- è¿™é‡Œä½ å¯ä»¥æ›¿æ¢ä¸ºä½ çš„Base64ç¼–ç çš„å¤´åƒå›¾ç‰‡
    -- ç”±äºŽRobloxçš„é™åˆ¶ï¼ŒBase64ä¸èƒ½ç›´æŽ¥ä½¿ç”¨ï¼Œéœ€è¦è½¬æ¢ä¸ºrbxassetid
    -- è¿™é‡Œæˆ‘ä»¬ä½¿ç”¨ä¸€ä¸ªç®€å•çš„å ä½ç¬¦ï¼Œä½ å¯ä»¥æ›¿æ¢ä¸ºä½ çš„å¤´åƒ
    
    -- åˆ›å»ºå¤´åƒImageLabel
    local AvatarImage = Instance.new("ImageLabel")
    AvatarImage.Name = "AvatarImage"
    AvatarImage.Size = UDim2.new(1, 0, 1, 0)
    AvatarImage.BackgroundTransparency = 1
    AvatarImage.Image = "rbxassetid://1234567890"  -- å ä½IDï¼Œæ›¿æ¢ä¸ºä½ çš„å¤´åƒID
    
    -- å¦‚æžœBase64å¯ç”¨ï¼Œå¯ä»¥ä½¿ç”¨ä»¥ä¸‹ä»£ç è½¬æ¢ï¼š
    -- æ³¨æ„ï¼šè¿™éœ€è¦é¢å¤–çš„åº“å’Œæƒé™ï¼Œè¿™é‡Œåªåšç¤ºä¾‹
    local function ConvertBase64ToImage(base64String)
        -- è¿™é‡Œåº”è¯¥æ˜¯å°†base64è½¬æ¢ä¸ºImageçš„ä»£ç 
        -- ç”±äºŽRobloxé™åˆ¶ï¼Œè¿™é€šå¸¸éœ€è¦å¤–éƒ¨æœåŠ¡
        return "rbxassetid://0"  -- è¿”å›žé»˜è®¤
    end
    
    -- å¤´åƒåœ†è§’
    local AvatarImageCorner = Instance.new("UICorner")
    AvatarImageCorner.CornerRadius = UDim.new(0, 12)
    AvatarImageCorner.Parent = AvatarImage
    
    -- å¤´åƒè¾¹æ¡†
    local AvatarStroke = Instance.new("UIStroke")
    AvatarStroke.Color = MD3Colors.Primary
    AvatarStroke.Thickness = 2
    AvatarStroke.Parent = AvatarImage
    
    -- æ›¿æ¢åŽŸæ¥çš„æ–‡å­—å¤´åƒ
    AvatarText:Destroy()
    AvatarImage.Parent = AvatarFrame
    
    -- å¤´åƒç‚¹å‡»æ•ˆæžœ
    AvatarImage.MouseButton1Click:Connect(function()
        local scaleUp = TweenService:Create(AvatarImage, TweenInfo.new(0.1), {
            Size = UDim2.new(1.2, 0, 1.2, 0)
        })
        
        local scaleDown = TweenService:Create(AvatarImage, TweenInfo.new(0.1), {
            Size = UDim2.new(1, 0, 1, 0)
        })
        
        scaleUp:Play()
        scaleUp.Completed:Wait()
        scaleDown:Play()
        
        -- åˆ‡æ¢åˆ°ä¸»é¡µ
        for btnName, btnData in pairs(NavButtons) do
            btnData.active = (btnName == "Home")
            btnData.button.Indicator.Visible = (btnName == "Home")
            
            if btnName == "Home" then
                local tween = TweenService:Create(btnData.button, TweenInfo.new(0.2), {
                    BackgroundTransparency = 0.2
                })
                tween:Play()
            else
                local tween = TweenService:Create(btnData.button, TweenInfo.new(0.2), {
                    BackgroundTransparency = 0.5
                })
                tween:Play()
            end
            
            if Pages[btnName] then
                Pages[btnName].Visible = (btnName == "Home")
            end
        end
        
        ActivePage = "Home"
    end)
end

-- åˆå§‹åŒ–å¤´åƒ
SetupAvatar()

-- æ€§èƒ½ä¼˜åŒ–
local Performance = {
    FrameCounter = 0,
    LastFPS = 60,
    LastUpdate = tick(),
    
    -- é™åˆ¶FPS
    LimitFPS = function(self, targetFPS)
        local currentTime = tick()
        local frameTime = 1 / targetFPS
        
        if self.FrameCounter % 60 == 0 then
            local elapsed = currentTime - self.LastUpdate
            self.LastFPS = math.floor(60 / elapsed)
            self.LastUpdate = currentTime
        end
        
        self.FrameCounter = self.FrameCounter + 1
    end,
    
    -- å†…å­˜æ¸…ç†
    CleanupMemory = function(self)
        collectgarbage("collect")
        print("å†…å­˜æ¸…ç†å®Œæˆ: " .. collectgarbage("count") .. "KB")
    end
}

-- å®šæœŸå†…å­˜æ¸…ç†
coroutine.wrap(function()
    while wait(60) do
        Performance:CleanupMemory()
    end
end)()

-- é”™è¯¯å¤„ç†
local ErrorHandler = {
    -- å®‰å…¨æ‰§è¡Œå‡½æ•°
    SafeExecute = function(self, func, errorMessage)
        local success, err = pcall(func)
        if not success then
            warn("é”™è¯¯: " .. errorMessage .. ": " .. tostring(err))
            return false
        end
        return true
    end,
    
    -- å…¨å±€é”™è¯¯æ•èŽ·
    SetupGlobalErrorHandler = function(self)
        local xpcall = xpcall or function(f, err)
            local ok, result = pcall(f)
            if not ok then
                err(result)
            end
            return ok, result
        end
        
        -- è¿™é‡Œå¯ä»¥æ·»åŠ æ›´å¤æ‚çš„é”™è¯¯å¤„ç†é€»è¾‘
    end
}

ErrorHandler:SetupGlobalErrorHandler()

-- UIç¾ŽåŒ–å¢žå¼º
local function EnhanceUI()
    -- ä¸ºå¯¼èˆªæŒ‰é’®æ·»åŠ æ¸å˜æ•ˆæžœ
    for _, btnData in pairs(NavButtons) do
        local button = btnData.button
        
        -- æ·»åŠ æŒ‰é’®æ¸å˜
        local buttonGradient = Instance.new("UIGradient")
        buttonGradient.Color = ColorSequence.new({
            ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 255, 255)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(240, 240, 240))
        })
        buttonGradient.Transparency = NumberSequence.new({
            NumberSequenceKeypoint.new(0, 0.7),
            NumberSequenceKeypoint.new(1, 0.9)
        })
        buttonGradient.Parent = button
        
        -- æŒ‰é’®é˜´å½±
        local buttonShadow = Instance.new("UIStroke")
        buttonShadow.Color = Color3.fromRGB(0, 0, 0, 30)
        buttonShadow.Thickness = 1
        buttonShadow.Transparency = 0.5
        buttonShadow.Parent = button
    end
    
    -- ä¸ºé¡µé¢æ·»åŠ è¿›å…¥åŠ¨ç”»
    for pageName, page in pairs(Pages) do
        page:GetPropertyChangedSignal("Visible"):Connect(function()
            if page.Visible then
                -- é¡µé¢è¿›å…¥åŠ¨ç”»
                page.CanvasPosition = Vector2.new(0, 0)
                
                for _, child in ipairs(page:GetDescendants()) do
                    if child:IsA("Frame") and child.BackgroundTransparency == 1 then
                        child.Position = UDim2.new(0, 20, 0, 0)
                        child.BackgroundTransparency = 1
                        
                        local tween = TweenService:Create(child, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                            Position = UDim2.new(0, 0, 0, 0),
                            BackgroundTransparency = 1
                        })
                        tween:Play()
                    end
                end
            end
        end)
    end
end

EnhanceUI()

-- çŠ¶æ€ç›‘æŽ§
local StatusMonitor = {
    LastUpdate = tick(),
    UpdateInterval = 5,  -- 5ç§’æ›´æ–°ä¸€æ¬¡
    
    Update = function(self)
        local currentTime = tick()
        if currentTime - self.LastUpdate < self.UpdateInterval then
            return
        end
        
        self.LastUpdate = currentTime
        
        -- æ›´æ–°çŠ¶æ€ä¿¡æ¯
        if Pages["Home"] and Pages["Home"].Visible then
            local homePage = Pages["Home"]
            local statusLabel = homePage:FindFirstChild("StatusLabel", true)
            
            if statusLabel then
                local fps = math.floor(1 / RunService.RenderStepped:Wait())
                local ping = "N/A"
                
                -- å°è¯•èŽ·å–ping
                pcall(function()
                    ping = math.floor(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue()) .. "ms"
                end)
                
                local statusText = string.format(
                    "âœ… é˜²è¸¢ç³»ç»Ÿ: å·²æ¿€æ´»\n" ..
                    "ðŸ›¡ï¸ ç™½åå•: å·²éªŒè¯\n" ..
                    "âš¡ ç³»ç»ŸçŠ¶æ€: æ­£å¸¸\n" ..
                    "ðŸŽ® FPS: %d\n" ..
                    "ðŸ“¶ å»¶è¿Ÿ: %s\n" ..
                    "ðŸ”§ è„šæœ¬ç‰ˆæœ¬: v3.0",
                    fps, ping
                )
                
                statusLabel.Text = statusText
            end
        end
    end
}

-- çŠ¶æ€æ›´æ–°å¾ªçŽ¯
coroutine.wrap(function()
    while wait(1) do
        StatusMonitor:Update()
    end
end)()

-- é…ç½®æ–‡ä»¶ç³»ç»Ÿ
local ConfigSystem = {
    Config = {},
    ConfigName = "ShiQian_Config",
    
    -- ä¿å­˜é…ç½®
    Save = function(self)
        self.Config = {
            Aim = {
                Enabled = AimConfig.Enabled,
                Smoothness = AimConfig.Smoothness,
                FOV = AimConfig.FOV,
                TeamCheck = AimConfig.TeamCheck,
                WallCheck = AimConfig.WallCheck,
                Prediction = AimConfig.Prediction
            },
            Weapon = {
                InfiniteAmmo = ACSFunctions.InfiniteAmmo,
                NoReload = ACSFunctions.NoReload,
                RapidFire = ACSFunctions.RapidFire,
                NoRecoil = ACSFunctions.NoRecoil,
                NoSpread = ACSFunctions.NoSpread
            },
            Movement = {
                HighJump = Movement.HighJump,
                InfiniteJump = Movement.InfiniteJump,
                Speed = Movement.Speed,
                JumpPowerValue = Movement.JumpPowerValue,
                SpeedValue = Movement.SpeedValue
            },
            Fly = {
                Enabled = Fly.Enabled,
                Speed = Fly.Speed,
                NoClip = Fly.NoClip
            },
            UI = {
                Position = {MainWindow.Position.X.Scale, MainWindow.Position.X.Offset, 
                           MainWindow.Position.Y.Scale, MainWindow.Position.Y.Offset}
            }
        }
        
        -- è¿™é‡Œå¯ä»¥æ·»åŠ å°†é…ç½®ä¿å­˜åˆ°æ–‡ä»¶æˆ–äº‘ç«¯çš„ä»£ç 
        -- ç”±äºŽRobloxé™åˆ¶ï¼Œé€šå¸¸éœ€è¦ä½¿ç”¨DataStoreæˆ–å¤–éƒ¨æœåŠ¡
        print("é…ç½®å·²ä¿å­˜")
    end,
    
    -- åŠ è½½é…ç½®
    Load = function(self)
        -- è¿™é‡Œåº”è¯¥ä»Žä¿å­˜çš„ä½ç½®åŠ è½½é…ç½®
        -- æš‚æ—¶ä½¿ç”¨é»˜è®¤å€¼
        
        print("é…ç½®å·²åŠ è½½")
    end
}

-- è‡ªåŠ¨ä¿å­˜é…ç½®
coroutine.wrap(function()
    while wait(30) do
        ConfigSystem:Save()
    end
end)()

-- è„šæœ¬å¸è½½æ¸…ç†
local function Cleanup()
    print("æ­£åœ¨æ¸…ç†è„šæœ¬...")
    
    -- åœæ­¢è‡ªçž„
    StopAim()
    
    -- ç¦ç”¨æ­¦å™¨åŠŸèƒ½
    if ACSFunctions.InfiniteAmmo then ACSFunctions:DisableInfiniteAmmo() end
    if ACSFunctions.NoReload then ACSFunctions:DisableNoReload() end
    if ACSFunctions.RapidFire then ACSFunctions:DisableRapidFire() end
    if ACSFunctions.NoRecoil then ACSFunctions:DisableNoRecoil() end
    if ACSFunctions.NoSpread then ACSFunctions:DisableNoSpread() end
    
    -- ç¦ç”¨ç§»åŠ¨åŠŸèƒ½
    if Movement.HighJump then Movement:DisableHighJump() end
    if Movement.InfiniteJump then Movement:DisableInfiniteJump() end
    if Movement.Speed then Movement:DisableSpeed() end
    
    -- ç¦ç”¨é£žè¡Œ
    if Fly.Enabled then Fly:DisableFly() end
    if Fly.NoClip then Fly:DisableNoClip() end
    
    -- ä¿å­˜é…ç½®
    ConfigSystem:Save()
    
    -- æ¸…ç†å†…å­˜
    Performance:CleanupMemory()
    
    print("è„šæœ¬æ¸…ç†å®Œæˆ")
end

-- çª—å£å…³é—­æ—¶æ¸…ç†
MainWindow:GetPropertyChangedSignal("Parent"):Connect(function()
    if not MainWindow.Parent then
        Cleanup()
    end
end)

-- çŽ©å®¶ç¦»å¼€æ—¶æ¸…ç†
game:BindToClose(function()
    Cleanup()
end)

-- æœ€ç»ˆä¼˜åŒ–
local FinalOptimization = {
    Apply = function(self)
        -- é™ä½ŽUIæ›´æ–°é¢‘çŽ‡
        for _, page in pairs(Pages) do
            if page:IsA("ScrollingFrame") then
                page.AutomaticCanvasSize = Enum.AutomaticSize.Y
                page.ScrollingEnabled = true
                page.ElasticBehavior = Enum.ElasticBehavior.Always
            end
        end
        
        -- ä¼˜åŒ–æ¸å˜æ€§èƒ½
        MainWindow.UIGradient.Enabled = true
        
        -- å¯ç”¨UI
        ScreenGui.Enabled = true
    end
}

FinalOptimization:Apply()

