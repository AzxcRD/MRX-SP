-- æœåŠ¡å¼•ç”¨
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local HttpService = game:GetService("HttpService")

-- æœ¬åœ°çŽ©å®¶
local LocalPlayer = Players.LocalPlayer
local Camera = Workspace.CurrentCamera

-- ç™½åå•ç³»ç»Ÿ
local Whitelist = {
    ["æ—¶è¿"] = true,  -- ç¤ºä¾‹ï¼Œä½ å¯ä»¥æ·»åŠ æ›´å¤š
    ["Admin"] = true,
    ["Developer"] = true
}

-- æ£€æŸ¥ç™½åå•
local function CheckWhitelist()
    local playerName = LocalPlayer.Name
    if Whitelist[playerName] then
        return true
    end
    
    -- æ£€æŸ¥ç”¨æˆ·ID
    local playerId = tostring(LocalPlayer.UserId)
    if Whitelist[playerId] then
        return true
    end
    
    return false
end

-- ç™½åå•æ£€æµ‹
if not CheckWhitelist() then
    print("âŒ ä¸åœ¨ç™½åå•ä¸­ï¼Œè„šæœ¬ç»ˆæ­¢")
    return
end

print("âœ… ç™½åå•éªŒè¯é€šè¿‡")

-- é˜²è¸¢ç³»ç»Ÿ
local function SetupAntiKick()
    print("ðŸ›¡ï¸ åˆå§‹åŒ–é˜²è¸¢ç³»ç»Ÿ...")
    
    -- Hook __namecall
    local mt = getrawmetatable(game)
    if mt then
        local old = mt.__namecall
        setreadonly(mt, false)
        
        mt.__namecall = newcclosure(function(...)
            local method = getnamecallmethod()
            local args = {...}
            
            -- é˜»æ­¢è¸¢å‡º
            if tostring(method) == "Kick" or tostring(method) == "kick" then
                warn("ðŸ›¡ï¸ é˜²è¸¢ç³»ç»Ÿæ‹¦æˆªäº†è¸¢å‡ºå°è¯•: " .. tostring(args[2]))
                return nil
            end
            
            -- é˜»æ­¢æ–­å¼€è¿žæŽ¥
            if tostring(method) == "Disconnect" then
                warn("ðŸ›¡ï¸ é˜²è¸¢ç³»ç»Ÿæ‹¦æˆªäº†æ–­å¼€è¿žæŽ¥å°è¯•")
                return function() end
            end
            
            return old(...)
        end)
        
        setreadonly(mt, true)
    end
    
    -- Hook Index
    pcall(function()
        local mt2 = getrawmetatable(game)
        if mt2 then
            local oldIndex = mt2.__index
            setreadonly(mt2, false)
            
            mt2.__index = newcclosure(function(t, k)
                if tostring(k) == "Kick" or tostring(k) == "kick" then
                    return function() warn("ðŸ›¡ï¸ é˜²è¸¢ç³»ç»Ÿæ‹¦æˆªäº†è¸¢å‡º") end
                end
                return oldIndex(t, k)
            end)
            
            setreadonly(mt2, true)
        end
    end)
    
    print("âœ… é˜²è¸¢ç³»ç»Ÿå·²åŠ è½½")
end

-- ç«‹å³å¯åŠ¨é˜²è¸¢
SetupAntiKick()

-- ç»†è…»çš„å¼€åœºåŠ¨ç”»
local function ShowIntroAnimation()
    print("ðŸŽ¬ å¼€å§‹å¼€åœºåŠ¨ç”»...")
    
    -- åˆ›å»ºå…¨å±è¦†ç›–
    local IntroGui = Instance.new("ScreenGui")
    IntroGui.Name = "IntroGui"
    IntroGui.ResetOnSpawn = false
    IntroGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    IntroGui.DisplayOrder = 1000
    
    local Background = Instance.new("Frame")
    Background.Name = "Background"
    Background.Size = UDim2.new(1, 0, 1, 0)
    Background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Background.BackgroundTransparency = 0
    Background.BorderSizePixel = 0
    Background.Parent = IntroGui
    
    -- ä¸»æ ‡é¢˜
    local TitleText = Instance.new("TextLabel")
    TitleText.Name = "TitleText"
    TitleText.AnchorPoint = Vector2.new(0.5, 0.5)
    TitleText.Size = UDim2.new(0, 0, 0, 0)
    TitleText.Position = UDim2.new(0.5, 0, 0.4, 0)
    TitleText.BackgroundTransparency = 1
    TitleText.Text = "æ—¶è¿"
    TitleText.TextColor3 = Color3.fromRGB(255, 255, 255)
    TitleText.TextSize = 1
    TitleText.Font = Enum.Font.GothamBold
    TitleText.TextTransparency = 1
    TitleText.Parent = IntroGui
    
    -- å‰¯æ ‡é¢˜
    local SubText = Instance.new("TextLabel")
    SubText.Name = "SubText"
    SubText.AnchorPoint = Vector2.new(0.5, 0.5)
    SubText.Size = UDim2.new(0, 0, 0, 0)
    SubText.Position = UDim2.new(0.5, 0, 0.6, 0)
    SubText.BackgroundTransparency = 1
    SubText.Text = "é«˜çº§åŠŸèƒ½ç³»ç»Ÿ"
    SubText.TextColor3 = Color3.fromRGB(180, 180, 180)
    SubText.TextSize = 1
    SubText.Font = Enum.Font.Gotham
    SubText.TextTransparency = 1
    SubText.Parent = IntroGui
    
    -- åˆ›å»ºç´«è‰²çˆªç—•
    local clawParts = {}
    local clawPoints = {
        -- ä¸»çˆª
        {0.3, 0.3, 0.7, 0.7},
        {0.4, 0.2, 0.6, 0.8},
        {0.2, 0.4, 0.8, 0.6},
        -- ç»†èŠ‚çˆªç—•
        {0.35, 0.25, 0.65, 0.75},
        {0.25, 0.35, 0.75, 0.65},
    }
    
    for i, points in ipairs(clawPoints) do
        local claw = Instance.new("Frame")
        claw.Name = "Claw" .. i
        claw.AnchorPoint = Vector2.new(0.5, 0.5)
        claw.Size = UDim2.new(0, 0, 0, 2)
        claw.Position = UDim2.new(0.5, 0, 0.5, 0)
        claw.BackgroundColor3 = Color3.fromRGB(103, 80, 164)
        claw.BackgroundTransparency = 0.3
        claw.BorderSizePixel = 0
        claw.Rotation = 45
        claw.Parent = IntroGui
        
        table.insert(clawParts, claw)
    end
    
    IntroGui.Parent = game:GetService("CoreGui")
    
    -- ç¬¬ä¸€é˜¶æ®µï¼šæ·¡å…¥æ–‡å­—
    wait(0.5)
    
    local textFadeIn = TweenService:Create(TitleText, TweenInfo.new(1, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
        TextTransparency = 0
    })
    
    textFadeIn:Play()
    
    wait(0.3)
    
    local subFadeIn = TweenService:Create(SubText, TweenInfo.new(1, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
        TextTransparency = 0
    })
    
    subFadeIn:Play()
    
    -- ç¬¬äºŒé˜¶æ®µï¼šæ–‡å­—ç¼©æ”¾
    wait(0.5)
    
    local textScale = TweenService:Create(TitleText, TweenInfo.new(1.2, Enum.EasingStyle.Elastic, Enum.EasingDirection.Out), {
        TextSize = 80,
        Size = UDim2.new(0, 300, 0, 120)
    })
    
    local subScale = TweenService:Create(SubText, TweenInfo.new(1, Enum.EasingStyle.Back, Enum.EasingDirection.Out, 0.2), {
        TextSize = 24,
        Size = UDim2.new(0, 250, 0, 40)
    })
    
    textScale:Play()
    subScale:Play()
    
    wait(1)
    
    -- ç¬¬ä¸‰é˜¶æ®µï¼šçˆªç—•åŠ¨ç”»
    for i, claw in ipairs(clawParts) do
        local delay = (i-1) * 0.1
        wait(delay)
        
        local clawExpand = TweenService:Create(claw, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
            Size = UDim2.new(0.3, 0, 0, 2),
            Rotation = claw.Rotation + (i % 2 == 0 and 10 or -10)
        })
        
        local clawFade = TweenService:Create(claw, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            BackgroundTransparency = 0.8
        })
        
        clawExpand:Play()
        clawFade:Play()
    end
    
    wait(0.5)
    
    -- ç¬¬å››é˜¶æ®µï¼šæ–‡å­—åˆ†ç¦»
    local titleMove = TweenService:Create(TitleText, TweenInfo.new(0.8, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
        Position = UDim2.new(0.3, 0, 0.4, 0)
    })
    
    local subMove = TweenService:Create(SubText, TweenInfo.new(0.8, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
        Position = UDim2.new(0.7, 0, 0.4, 0)
    })
    
    titleMove:Play()
    subMove:Play()
    
    wait(0.8)
    
    -- ç¬¬äº”é˜¶æ®µï¼šèƒŒæ™¯æ·¡å‡º
    local bgFade = TweenService:Create(Background, TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        BackgroundTransparency = 1
    })
    
    local textFadeOut = TweenService:Create(TitleText, TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        TextTransparency = 1
    })
    
    local subFadeOut = TweenService:Create(SubText, TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        TextTransparency = 1
    })
    
    -- çˆªç—•æ·¡å‡º
    for _, claw in ipairs(clawParts) do
        local clawFadeOut = TweenService:Create(claw, TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            BackgroundTransparency = 1
        })
        clawFadeOut:Play()
    end
    
    bgFade:Play()
    textFadeOut:Play()
    subFadeOut:Play()
    
    bgFade.Completed:Wait()
    IntroGui:Destroy()
    
    print("âœ… å¼€åœºåŠ¨ç”»å®Œæˆ")
end

-- æ˜¾ç¤ºé˜²è¸¢æç¤º
local function ShowAntiKickNotification()
    local NotifyGui = Instance.new("ScreenGui")
    NotifyGui.Name = "AntiKickNotify"
    NotifyGui.ResetOnSpawn = false
    NotifyGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    NotifyGui.DisplayOrder = 999
    
    local NotifyFrame = Instance.new("Frame")
    NotifyFrame.Name = "NotifyFrame"
    NotifyFrame.AnchorPoint = Vector2.new(1, 1)
    NotifyFrame.Size = UDim2.new(0, 300, 0, 60)
    NotifyFrame.Position = UDim2.new(1, -20, 1, -20)
    NotifyFrame.BackgroundColor3 = Color3.fromRGB(103, 80, 164)
    NotifyFrame.BackgroundTransparency = 0.2
    NotifyFrame.BorderSizePixel = 0
    
    local UICorner = Instance.new("UICorner")
    UICorner.CornerRadius = UDim.new(0, 12)
    UICorner.Parent = NotifyFrame
    
    local UIStroke = Instance.new("UIStroke")
    UIStroke.Color = Color3.fromRGB(150, 120, 200)
    UIStroke.Thickness = 2
    UIStroke.Parent = NotifyFrame
    
    local NotifyLabel = Instance.new("TextLabel")
    NotifyLabel.Name = "NotifyLabel"
    NotifyLabel.Size = UDim2.new(1, -20, 1, -20)
    NotifyLabel.Position = UDim2.new(0, 10, 0, 10)
    NotifyLabel.BackgroundTransparency = 1
    NotifyLabel.Text = "ðŸ›¡ï¸ é˜²è¸¢ç³»ç»Ÿå·²æ¿€æ´»\nå·²å…ç–«æœåŠ¡å™¨è¸¢å‡º"
    NotifyLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    NotifyLabel.TextSize = 16
    NotifyLabel.Font = Enum.Font.GothamSemibold
    NotifyLabel.TextXAlignment = Enum.TextXAlignment.Left
    NotifyLabel.TextYAlignment = Enum.TextYAlignment.Top
    NotifyLabel.TextWrapped = true
    NotifyLabel.Parent = NotifyFrame
    
    NotifyFrame.Parent = NotifyGui
    NotifyGui.Parent = game:GetService("CoreGui")
    
    -- åŠ¨ç”»ï¼šä»Žå³ä¾§æ»‘å…¥
    NotifyFrame.Position = UDim2.new(1, 320, 1, -20)
    NotifyFrame.BackgroundTransparency = 1
    
    local slideIn = TweenService:Create(NotifyFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Position = UDim2.new(1, -20, 1, -20),
        BackgroundTransparency = 0.2
    })
    
    slideIn:Play()
    
    wait(3)
    
    -- åŠ¨ç”»ï¼šæ·¡å‡º
    local fadeOut = TweenService:Create(NotifyFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Position = UDim2.new(1, 320, 1, -20),
        BackgroundTransparency = 1
    })
    
    fadeOut:Play()
    fadeOut.Completed:Wait()
    NotifyGui:Destroy()
end

-- åŠŸèƒ½å¼€å¯æç¤º
local function ShowFunctionNotification(title, message)
    local NotifyGui = Instance.new("ScreenGui")
    NotifyGui.Name = "FunctionNotify"
    NotifyGui.ResetOnSpawn = false
    NotifyGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    NotifyGui.DisplayOrder = 999
    
    local NotifyFrame = Instance.new("Frame")
    NotifyFrame.Name = "NotifyFrame"
    NotifyFrame.AnchorPoint = Vector2.new(1, 1)
    NotifyFrame.Size = UDim2.new(0, 280, 0, 80)
    NotifyFrame.Position = UDim2.new(1, -20, 1, -20)
    NotifyFrame.BackgroundColor3 = Color3.fromRGB(103, 80, 164)
    NotifyFrame.BackgroundTransparency = 0.2
    NotifyFrame.BorderSizePixel = 0
    
    local UICorner = Instance.new("UICorner")
    UICorner.CornerRadius = UDim.new(0, 12)
    UICorner.Parent = NotifyFrame
    
    local UIStroke = Instance.new("UIStroke")
    UIStroke.Color = Color3.fromRGB(150, 120, 200)
    UIStroke.Thickness = 2
    UIStroke.Parent = NotifyFrame
    
    local TitleLabel = Instance.new("TextLabel")
    TitleLabel.Name = "TitleLabel"
    TitleLabel.Size = UDim2.new(1, -20, 0, 25)
    TitleLabel.Position = UDim2.new(0, 10, 0, 10)
    TitleLabel.BackgroundTransparency = 1
    TitleLabel.Text = "ðŸŽ¯ " .. title
    TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TitleLabel.TextSize = 18
    TitleLabel.Font = Enum.Font.GothamBold
    TitleLabel.TextXAlignment = Enum.TextXAlignment.Left
    TitleLabel.TextYAlignment = Enum.TextYAlignment.Top
    TitleLabel.Parent = NotifyFrame
    
    local MessageLabel = Instance.new("TextLabel")
    MessageLabel.Name = "MessageLabel"
    MessageLabel.Size = UDim2.new(1, -20, 0.6, 0)
    MessageLabel.Position = UDim2.new(0, 10, 0, 40)
    MessageLabel.BackgroundTransparency = 1
    MessageLabel.Text = message
    MessageLabel.TextColor3 = Color3.fromRGB(220, 220, 220)
    MessageLabel.TextSize = 14
    MessageLabel.Font = Enum.Font.Gotham
    MessageLabel.TextXAlignment = Enum.TextXAlignment.Left
    MessageLabel.TextYAlignment = Enum.TextYAlignment.Top
    MessageLabel.TextWrapped = true
    MessageLabel.Parent = NotifyFrame
    
    NotifyFrame.Parent = NotifyGui
    NotifyGui.Parent = game:GetService("CoreGui")
    
    -- åŠ¨ç”»ï¼šä»Žå³ä¾§æ»‘å…¥
    NotifyFrame.Position = UDim2.new(1, 320, 1, -20)
    NotifyFrame.BackgroundTransparency = 1
    
    local slideIn = TweenService:Create(NotifyFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Position = UDim2.new(1, -20, 1, -20),
        BackgroundTransparency = 0.2
    })
    
    slideIn:Play()
    
    -- 3ç§’åŽè‡ªåŠ¨æ¶ˆå¤±
    delay(3, function()
        local fadeOut = TweenService:Create(NotifyFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Position = UDim2.new(1, 320, 1, -20),
            BackgroundTransparency = 1
        })
        
        fadeOut:Play()
        fadeOut.Completed:Wait()
        NotifyGui:Destroy()
    end)
end

-- æ‰§è¡Œå¼€åœºåŠ¨ç”»
coroutine.wrap(ShowIntroAnimation)()

-- å»¶è¿Ÿæ˜¾ç¤ºé˜²è¸¢æç¤º
delay(2, function()
    ShowAntiKickNotification()
end)

wait(3)  -- ç­‰å¾…åŠ¨ç”»å®Œæˆ

