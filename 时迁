local MD3Colors = {
    Primary = Color3.fromRGB(103, 80, 164),      -- ä¸»è‰²è°ƒ - ç´«è‰²
    PrimaryContainer = Color3.fromRGB(234, 221, 255),  -- ä¸»å®¹å™¨
    OnPrimary = Color3.fromRGB(255, 255, 255),   -- ä¸»è‰²ä¸Šçš„æ–‡å­—
    Surface = Color3.fromRGB(255, 255, 255, 0.9),-- è¡¨é¢
    SurfaceVariant = Color3.fromRGB(230, 224, 233),  -- è¡¨é¢å˜ä½“
    Outline = Color3.fromRGB(121, 116, 126, 0.4),-- è½®å»“
    OutlineVariant = Color3.fromRGB(194, 189, 198),  -- è½®å»“å˜ä½“
    Error = Color3.fromRGB(186, 26, 26),         -- é”™è¯¯
    OnSurface = Color3.fromRGB(28, 27, 31),      -- è¡¨é¢ä¸Šçš„æ–‡å­—
    OnSurfaceVariant = Color3.fromRGB(73, 69, 78)  -- è¡¨é¢å˜ä½“ä¸Šçš„æ–‡å­—
}

