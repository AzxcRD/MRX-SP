-- æ‹–æ‹½åŠŸèƒ½
local Dragging = false
local DragStart, StartPos
local OriginalSize = MainWindow.Size
local DragScaleFactor = 1.05  -- æ‹–æ‹½æ—¶æ”¾å¤§5%

-- æ•´ä¸ªçª—å£éƒ½å¯æ‹–æ‹½
MainWindow.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        Dragging = true
        DragStart = UserInputService:GetMouseLocation()
        StartPos = Vector2.new(MainWindow.AbsolutePosition.X, MainWindow.AbsolutePosition.Y)
        MainWindow.ZIndex = 1000
        
        -- æ‹–æ‹½æ—¶æ”¾å¤§åŠ¨ç”»
        local scaleTween = TweenService:Create(MainWindow, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Size = OriginalSize * DragScaleFactor
        })
        scaleTween:Play()
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if Dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local mousePos = UserInputService:GetMouseLocation()
        local delta = mousePos - DragStart
        local newPos = StartPos + delta
        
        local screenSize = Camera.ViewportSize
        local windowSize = MainWindow.AbsoluteSize
        newPos = Vector2.new(
            math.clamp(newPos.X, 0, screenSize.X - windowSize.X),
            math.clamp(newPos.Y, 0, screenSize.Y - windowSize.Y)
        )
        
        MainWindow.Position = UDim2.new(0, newPos.X, 0, newPos.Y)
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        Dragging = false
        MainWindow.ZIndex = 1
        
        -- æ‹–æ‹½ç»“æŸæ—¶æ¢å¤å¤§å°
        local scaleTween = TweenService:Create(MainWindow, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Size = OriginalSize
        })
        scaleTween:Play()
    end
end)

-- çª—å£æŽ§åˆ¶æŒ‰é’®
